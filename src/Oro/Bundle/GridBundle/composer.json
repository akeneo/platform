{
    "name": "oro/grid-bundle",
    "type": "symfony-bundle",
    "description": "BAP Grid Bundle",
    "keywords": ["BAP", "UI", "User Interface", "Grid"],
    "homepage": "https://github.com/laboro/GridBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.1",
        "oro/ui-bundle": "dev-master",
        "oro/filter-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/locale-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\GridBundle": "" }
    },
    "target-dir": "Oro/Bundle/GridBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
